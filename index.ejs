            </div>
            <div class="OutputItem">
                Humidity: 
                <a id="humidity">--</a>%
            </div>
            <div class="OutputItem">
                Temperature: 
                <a id="temp">--</a>C
            </div>
            <div class="OutputItem">
                Temperature Minimum: 
                <a id="tempMin">--</a>C
            </div>
            <div class="OutputItem">
                Temperature Maximum: 
                <a id="tempMax">--</a>C
            </div>

            <div class="OutputItem">
                Wind speeds up to <a id="wind">--</a><a id="gust">--</a>mph.
            </div>
        </div>
